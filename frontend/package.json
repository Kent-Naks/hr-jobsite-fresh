{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": { "node": "20.x" },


  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  },

  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:vercel": "prisma generate --schema prisma/schema.prisma && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",

    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:seed": "prisma db seed",
    "studio": "prisma studio",

    "import:json": "node scripts/import-json-to-db.mjs",


    "postinstall": "prisma generate --schema prisma/schema.prisma"
  },

  "dependencies": {
    "@prisma/client": "^6.15.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
